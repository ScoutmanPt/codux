{
    "$schema": "https://wixplosives.github.io/codux-config-schema/codux.config.schema.json",
    "newComponent": {
        "componentsPath": "src/newcompos"
    } ,
    "componentsDiscovery": {
        "include": [
            "src/**"
        ],
        "exclude": [
            "src/_codux/component-templates/***"
        ]
    },
    "compilerRuntime": "sass",
    "resolveUrls": false,
    "includePaths": [
        "src/**"
    ],
    "resolve": {
        "alias": {
          "@ms/odsp-core-bundle": false
        }
      },
    "boardGlobalSetup": "./src/_codux/board-global-setup.ts"
}